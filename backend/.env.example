# Backend environment variables
# Update and copy to .env in the same folder
DATABASE_URL=postgresql://postgres:postgres@db:5432/postgres

# Upstash Redis REST API credentials
# Get these from your Upstash Redis console
UPSTASH_REDIS_REST_URL=https://your-redis-name.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-token-here

# Upstash Redis URL for Celery (redis:// protocol)
# Format: redis://default:<password>@<host>:<port>
# Get this from Upstash console under "Redis Connect" > "redis-cli"
REDIS_URL=redis://default:your-password@your-redis-name.upstash.io:6379
CELERY_BROKER_URL=redis://default:your-password@your-redis-name.upstash.io:6379
CELERY_RESULT_BACKEND=redis://default:your-password@your-redis-name.upstash.io:6379

SECRET_KEY=dev-secret
CSV_BATCH_SIZE=5000
MAX_UPLOAD_BYTES=5368709120
